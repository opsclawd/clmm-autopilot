{
  "name": "clmm-autopilot",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": "22.22.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -C apps/web build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "e2e:devnet": "pnpm -C packages/solana e2e:devnet",
    "positions:devnet": "node ./scripts/find-devnet-whirlpool-positions.mjs",
    "check:anchor-tooling": "node ./scripts/check-anchor-tooling.mjs"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "0.32.1",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "ts-mocha": "^10.0.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10"
  }
}
